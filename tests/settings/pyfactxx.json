{
  "logging": {
    "console-sink": { "level": "debug" },
    "file-sink": { "level": "debug" }
  },
  "semantic-web": {
    "prefixes": [
      { "alias": "pizza", "uri":  "http://www.co-ode.org/ontologies/pizza/pizza.owl" }
    ]
  },
  "data-sources": [
    {
      "path": "owl/test/pizza.owl",
      "language": "owl",
      "format": "xml"
    }
  ],
  "data-backends": [
    {
      "name": "pl",
      "type": "Prolog:rdf_db"
    },
    {
      "name": "factxx",
      "type": "FactxxBackend",
      "module": "reasoner/owl/FactxxReasoner.py"
    }
  ],
  "reasoner": [
    {
      "name": "factxx",
      "type": "FactxxReasoner",
      "module": "reasoner/owl/FactxxReasoner.py",
      "data-backend": "factxx"
    }
  ]
}
