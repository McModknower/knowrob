{
  "logging": {
    "console-sink": { "level": "debug" },
    "file-sink": { "level": "debug" }
  },
  "semantic-web": {
    "prefixes": [
      { "alias": "lpn", "uri":  "http://knowrob.org/kb/lpn" }
    ]
  },
  "data-sources": [
    {
      "path": "tests/owl/lpn.owl",
      "format": "turtle"
    }
  ],
  "data-backends": [
    {
      "type": "MongoDB",
      "name": "mongodb",
      "host": "localhost",
      "port": 27017,
      "db": "mongolog1",
      "read-only": false
    }
  ],
  "reasoner": [
    {
      "type": "LPNReasoner",
      "name": "lpn",
      "module": "tests/py/lpn_reasoner.py",
      "data-backend": "mongodb"
    }
  ]
}
