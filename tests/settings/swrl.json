{
  "logging": {
    "console-sink": { "level": "debug" },
    "file-sink": { "level": "debug" }
  },
  "semantic-web": {
    "prefixes": [
      { "alias": "swrl", "uri":  "http://knowrob.org/kb/swrl_test" }
    ]
  },
  "data-sources": [
    {
      "path": "owl/test/swrl.owl",
      "format": "rdf-xml"
    }
  ],
  "data-backends": [
    {
      "name": "pl",
      "type": "Prolog:rdf_db"
    }
  ],
  "reasoner": [
    {
      "name": "swrl-pl",
      "type": "SWRL",
      "data-backend": "pl",
      "imports": [
        {
          "path": "tests/swrl/test.swrl",
          "format": "pl-swrl"
        }
      ]
    }
  ]
}
